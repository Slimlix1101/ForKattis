#include <iostream>
#include <sstream>
#include <cstring>
using namespace std;

int *status;
bool *ob;

int main()
{
	string t;
	stringstream ss;
	int n, row, col;
	cin >> n;
	while (n--)
	{
		cin >> row >> col;
		status = new int[col];
		for (int i=0; i<row; i++)
		{	
			ob = new bool[col];
			memset(ob, false, sizeof(bool)*col);
			getline(cin, t);
			ss << t;
			while (ss >> t)
			{
				ob[t[0]-'1'] = true;
			}
			for (int k=1; k<col; k++)
				status[k] = (ob[k-1]? 0 : status[k-1]) + status[k];
		}
		cout << status[col-1] << endl;
		if (n != 0) cout << '\n';
			
	}
}
