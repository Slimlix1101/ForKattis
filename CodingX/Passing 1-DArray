#include <iostream>
using namespace std;

void onedimension(int *, int);

int main(void)
{
	int a[] = {1,2,3,4,5};
	int i;
	F(a, 5);
	for (int i = 0  ;i <5 ;i++)
		cout << a[i] << endl;
	return 0;
}
void onedimension(int intPtr[], int size) // call by reference, *intPtr is also acceptable
{
	int i;
	for (int i=0; i<size; i++)
		intPtr[i] *=5;
}
